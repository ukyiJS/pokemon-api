{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "",
  "author": "ukyi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "preserve": "npm run config && npm run build",
    "serve": "sls offline start",
    "predeploy": "cross-env NODE_ENV=production npm run config && npm run build",
    "deploy": "sls deploy --verbose",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "config": "ts-node -r tsconfig-paths/register src/utils/createEnvJson"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.4",
    "apollo-server-express": "^2.19.0",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.3.8",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-mongodb-session": "^2.4.1",
    "dotenv": "^8.2.0",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "hangul-js": "^0.2.6",
    "mongodb": "^3.6.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "0.2.26",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.2",
    "@nestjs/schematics": "^7.2.1",
    "@nestjs/testing": "^7.5.1",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/connect-mongodb-session": "^2.4.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "envalid": "^6.0.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "serverless": "^2.11.0",
    "serverless-offline": "6.4.0",
    "serverless-plugin-typescript": "^1.1.9",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
